{
  "name": "issue-reopener",
  "version": "1.2.0",
  "private": true,
  "description": "Reopen issues that are referenced by TODOs",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "package": "ncc build --external encoding --source-map",
    "lint": "eslint --max-warnings 0 src/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/tool-cache": "^2.0.1",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.15",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.57.0",
    "eslint-plugin-github": "4.10.2",
    "eslint-plugin-prettier": "5.1.3",
    "markdownlint-cli": "0.37.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.1",
    "typescript": "5.3.3"
  }
}
