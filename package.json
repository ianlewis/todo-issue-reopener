{
  "name": "issue-reopener",
  "version": "1.2.0",
  "type": "module",
  "private": true,
  "description": "Reopen issues that are referenced by TODOs",
  "main": "lib/main.js",
  "engines": {
    "node": ">=22.0.0 <23"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "package": "ncc build --external encoding --source-map",
    "lint": "eslint --max-warnings 0 src/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/tool-cache": "^2.0.1",
    "uuid": "^10.0.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-ts": "^3.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "22.1.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "^9.19.0",
    "eslint-plugin-github": "^5.1.8",
    "eslint-plugin-prettier": "^5.2.3",
    "markdownlint-cli": "^0.44.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.4",
    "typescript": "^5.7.3"
  }
}
